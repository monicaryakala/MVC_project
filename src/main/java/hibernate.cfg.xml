<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>       
  <session-factory>
    <property name="dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="show_sql">true</property> 
    <property name="hbm2ddl.import_files">import.sql</property>
    <mapping resource="com/albany/edu/fwp/model/Student.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/FoodItems.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/FoodSelected.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/Images.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/MealCourse.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/QuadInfo.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/FoodDate.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/ManagerInfo.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/FeedBack.hbm.xml" />
    <mapping resource="com/albany/edu/fwp/model/Config.hbm.xml" />
  </session-factory>
</hibernate-configuration> 